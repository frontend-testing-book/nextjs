{
  "name": "frontend-testing-book-nextjs",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.9.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "format": "eslint --fix --ext .ts,.tsx,.js ./src",
    "test:storybook": "test-storybook --url http://localhost:6006",
    "test:e2e": "npx playwright test",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:codegen": "npx playwright codegen",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier \"./src/**/*.ts\" \"./src/**/*.tsx\" --check",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "npx http-server storybook-static -a localhost -p 6006 -s",
    "storybook:test": "npx wait-on http://localhost:6006 && pnpm run test:storybook",
    "storybook:ci": "npx concurrently -k -s first \"pnpm run storybook:serve\" \"pnpm run storybook:test\"",
    "storycap": "npx storycap http://localhost:6006",
    "vrt:snapshot": "storycap --serverCmd \"npx http-server storybook-static -a localhost -p 6006\" http://localhost:6006",
    "vrt:expected": "rimraf .reg/expected && mv __screenshots__ .reg/expected",
    "vrt:run": "reg-suit run",
    "vrt:serve": "npx http-server .reg -o -p 8080",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:studio": "npx prisma studio",
    "docker:prod:build": "docker compose -f docker-compose.prod.yaml build",
    "docker:prod:start": "docker compose -f docker-compose.prod.yaml up",
    "docker:e2e:start": "pnpm run prisma:reset && start-server-and-test 'pnpm start' 3000 'pnpm run test:e2e'",
    "docker:e2e:build": "docker compose -f docker-compose.e2e.yaml build",
    "docker:e2e:ci": "docker compose -f docker-compose.e2e.yaml up --exit-code-from e2e",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@prisma/client": "^4.6.0",
    "@storybook/addon-viewport": "^7.6.6",
    "@storybook/csf": "^0.1.2",
    "aws-sdk": "^2.1251.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "concurrently": "^8.2.2",
    "connect-redis": "^6.1.3",
    "ioredis": "^5.2.4",
    "mime-types": "^2.1.35",
    "next": "^13.0.2",
    "next-session": "^4.0.5",
    "prisma": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.2",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "uuid": "^9.0.0",
    "wait-on": "^7.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-a11y": "^7.6.6",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^6.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.18",
    "@types/jest": "^29.2.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.8.5",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "axe-playwright": "^1.1.11",
    "babel-loader": "^8.2.5",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^5.7.2",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-html-reporters": "^3.0.11",
    "lint-staged": "^15.2.0",
    "msw": "^0.47.4",
    "msw-storybook-addon": "^1.10.0",
    "next-router-mock": "^0.9.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.40.1",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "reg-keygen-git-hash-plugin": "^0.12.1",
    "reg-notify-github-plugin": "^0.12.1",
    "reg-publish-s3-plugin": "^0.12.1",
    "reg-suit": "^0.12.1",
    "start-server-and-test": "^1.14.0",
    "storybook": "^7.6.6",
    "storycap": "^3.1.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/index.ts"
  },
  "overrides": {
    "storybook-addon-next-router": {
      "next": "$next"
    }
  },
  "volta": {
    "node": "21.5.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
